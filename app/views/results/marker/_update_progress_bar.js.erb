<script>
  function update_bar(score, outof) {
      var width = (score/outof) * 100;
      var $bar = $('#progress_span');
      $bar.text(score + '/' + outof + ' Completed');
      $bar.css('width', width + '%');
      if(width > 75) {
          $bar.css('background-color', 'green')
      }else if(width > 35) {
          $bar.css('background-color', '#FBC02D')
      }
  }
</script>
